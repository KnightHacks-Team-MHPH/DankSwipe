.row
  %h1 My Memes
.row
  = link_to "New Meme", new_meme_path, class: "btn btn-success"
.row
  %table.table
    %thead
      %th Meme Url
      %th Meme Pic
      %th Actions
    %tbody
      - @memes.each do |meme|
        %tr
          %td= meme.meme_url
          %td= image_tag meme.meme_url, size: "200x200"
          %td= link_to "Remove", meme_path(meme.id), method: :delete, class: "btn btn-danger"
        
